{
    "name": "cooltronicpl/varnishcache",
    "description": "Boost the performance of your site with Cloudflare or Varnish.",
    "type": "craft-plugin",
    "version": "3.1.0",
    "keywords": [
        "craft-plugin",
        "varnish-cache",
        "preload",
        "cloudflare",
        "html-purge",
        "performance-optimization",
        "sitemap-warmup",
        "cdn-reverse-proxy"
    ],
    "support": {
        "docs": "https://github.com/cooltronicpl/varnishcache/blob/master/README.md",
        "issues": "https://github.com/cooltronicpl/varnishcache/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "CoolTRONIC.pl sp. z o.o. (LLC)",
            "homepage": "https://cooltronic.pl",
            "role": "Maintainer"
        },
        {
            "name": "Pawel Potacki",
            "homepage": "https://potacki.com",
            "role": "Developer"
        }
    ],
    "require": {
        "craftcms/cms": "^5.0.0|^5.0.0.alpha|^5.0.0-beta|~5.0|^4.0.0|^3.0.0",
        "ext-curl": "*",
        "ext-simplexml": "*"
    },
    "autoload": {
        "psr-4": {
            "cooltronicpl\\varnishcache\\": "src/"
        }
    },
    "extra": {
        "name": "CDN Cache & Preload",
        "handle": "varnishcache",
        "schemaVersion": "3.1.0",
        "hasCpSettings": true,
        "hasCpSection": false,
        "allowAnonymous": true,
        "class": "cooltronicpl\\varnishcache\\VarnishCache"
    }
}
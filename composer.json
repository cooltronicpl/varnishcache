{
    "name": "cooltronicpl/varnishcache",
    "description": "The Varnish Cache plugin for Craft CMS is an advanced tool that enhances your website's performance by generating static HTML files and optional purging the Varnish cache.",
    "type": "craft-plugin",
    "version": "1.1.2",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "html cache"
    ],
    "support": {
        "docs": "https://github.com/cooltronicpl/varnishcache/blob/master/README.md",
        "issues": "https://github.com/cooltronicpl/varnishcache/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "CoolTRONIC.pl",
            "homepage": "https://cooltronic.pl"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "cooltronicpl\\varnishcache\\": "src/"
        }
    },
    "extra": {
        "name": "Varnish Cache & Preload to HTML",
        "handle": "varnishcache",
        "schemaVersion": "1.0.0",
        "hasCpSettings": true,
        "hasCpSection": false,
        "class": "cooltronicpl\\varnishcache\\VarnishCache"
    }
}
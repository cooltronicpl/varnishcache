{
    "name": "cooltronicpl/varnishcache",
    "description": "Boost the performance of your site with Cloudflare or Varnish.",
    "type": "craft-plugin",
    "version": "2.5.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "html cache",
        "varnish",
        "preload",
        "cloudflare",
        "cache",
        "caching",
        "html",
        "purge",
        "optimize",
        "website",
        "performance",
        "preheat",
        "warmup",
        "sitemap",
        "speed",
        "cloudflare",
        "cdn",
        "varnish",
        "reverse proxy"
    ],
    "support": {
        "docs": "https://github.com/cooltronicpl/varnishcache/blob/master/README.md",
        "issues": "https://github.com/cooltronicpl/varnishcache/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "CoolTRONIC.pl sp. z o.o. (LLC)",
            "homepage": "https://cooltronic.pl",
            "role": "Maintainer"
        },
        {
            "name": "Pawel Potacki",
            "homepage": "https://potacki.com",
            "role": "Developer"
        }
    ],
    "require": {
      "craftcms/cms": "^4.0|^3.0",
      "ext-curl": "*",
      "ext-simplexml": "*"
    },
    "autoload": {
        "psr-4": {
            "cooltronicpl\\varnishcache\\": "src/"
        }
    },
    "extra": {
        "name": "CDN Cache & Preload",
        "handle": "varnishcache",
        "schemaVersion": "2.5.0",
        "hasCpSettings": true,
        "hasCpSection": false,
        "allowAnonymous": true,
        "class": "cooltronicpl\\varnishcache\\VarnishCache"
    }
}